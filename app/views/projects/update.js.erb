
<% if @check != nil %>

$('h3[data-id=<%= @project.id %>]').find('.fa').toggleClass('selected');

<% end %>

$('#basicModal-<%= @project.id %>').modal('hide')

$('[data-id=<%= @project.id %>]').children('a').children('.hover-mask').first().find('.overview').text('<%= @project.description %>')
$('[data-id=<%= @project.id %>]').children('a').children('.hover-mask').first().find('.need-help').text('<%= @project.need_help_with %>')